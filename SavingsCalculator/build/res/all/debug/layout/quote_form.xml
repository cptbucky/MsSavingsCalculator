<ScrollView xmlns:a="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            a:id="@+id/ScrollView1"
            a:layout_width="match_parent"
            a:layout_height="match_parent"
            a:layout_gravity="fill_vertical|fill_horizontal">

    <LinearLayout
            a:id="@+id/RelativeLayout1"
            a:layout_width="match_parent"
            a:layout_height="wrap_content"
            a:focusable="true"
            a:focusableInTouchMode="true"
            a:padding="10dp"
            a:orientation="vertical">

        <TextView
                a:layout_width="wrap_content"
                a:layout_height="wrap_content"
                a:text="@string/client_section_header"
                style="@style/sectionHeaderLabel"/>

        <View
                a:id="@+id/sepLine01"
                style="@style/underliningRule"
                a:layout_width="fill_parent"
                a:layout_height="1dp"/>

        <include
                layout="@layout/quote_incumbent_headline"
                a:id="@+id/incumbentHeadline"
                a:layout_width="fill_parent"
                a:layout_height="wrap_content"
                a:layout_marginLeft="20dp"
                a:layout_marginTop="10dp"/>

        <TextView
                a:layout_width="wrap_content"
                a:layout_height="wrap_content"
                a:text="@string/quote_calculation_section_heading"
                style="@style/sectionHeaderLabel"
                a:layout_marginTop="20dp"/>

        <View
                a:id="@+id/sepLine02"
                style="@style/underliningRule"
                a:layout_width="fill_parent"
                a:layout_height="1dp"/>

        <include
                layout="@layout/quote_calculation_inc_costing"
                a:id="@+id/quoteCalc"
                a:layout_width="fill_parent"
                a:layout_height="fill_parent"
                a:layout_marginLeft="20dp"
                a:layout_marginTop="15dp"/>

        <TextView
                a:layout_width="wrap_content"
                a:layout_height="wrap_content"
                a:text="@string/savings_section_header"
                style="@style/sectionHeaderLabel"
                a:layout_marginTop="20dp"/>

        <View
                a:id="@+id/sepLine03"
                style="@style/underliningRule"
                a:layout_width="fill_parent"
                a:layout_height="1dp"/>

        <fragment
                a:id="@+id/savingsFrag"
                a:name="com.avantics.savingscalc.common.fragments.SavingsFragment"
                a:layout_width="match_parent"
                a:layout_height="fill_parent"
                tools:layout="@layout/quote_savings"/>

    </LinearLayout>

</ScrollView><!-- From: file:/Users/tom/Dev/MsSavingsCalculator/SavingsCalculatorCommon/res/layout/quote_form.xml --><!-- From: file:/Users/tom/Dev/MsSavingsCalculator/SavingsCalculator/build/exploded-bundles/SavingsCalculatorSavingsCalculatorCommonUnspecified.aar/res/layout/quote_form.xml -->