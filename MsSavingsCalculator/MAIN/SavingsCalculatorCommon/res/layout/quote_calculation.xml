<RelativeLayout xmlns:a="http://schemas.android.com/apk/res/android"
                a:id="@+id/RelativeLayout1"
                a:layout_width="match_parent"
                a:layout_height="wrap_content"
                a:focusable="true"
                a:focusableInTouchMode="true">

    <LinearLayout
            a:id="@+id/LinearLayout3"
            a:layout_width="fill_parent"
            a:layout_height="wrap_content">

        <TextView
                a:id="@+id/spacer1"
                style="@style/calcRowLabel"
                a:layout_height="wrap_content"/>

        <TextView
                a:id="@+id/TextView14"
                style="@style/centerAlignLabel"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"
                a:text="@string/statement_total"/>

        <TextView
                a:id="@+id/lblVendorRate"
                style="@style/centerAlignLabel"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"
                a:text="@string/vendor_rate"/>

    </LinearLayout>

    <LinearLayout
            a:id="@+id/LinearLayout4"
            style="@style/linearLayoutRow"
            a:layout_width="fill_parent"
            a:layout_height="37sp"
            a:layout_below="@+id/LinearLayout3">

        <TextView
                a:id="@+id/TextView04"
                style="@style/calcRowLabel"
                a:layout_height="wrap_content"
                a:text="@string/cc_calculation_row_label"/>

        <EditText
                a:id="@+id/CCST"
                style="@style/currencyInput"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"/>

        <EditText
                a:id="@+id/CCFR"
                style="@style/percentageInput"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
            a:id="@+id/LinearLayout5"
            style="@style/linearLayoutRow"
            a:layout_width="fill_parent"
            a:layout_height="37sp"
            a:layout_below="@+id/LinearLayout4">

        <TextView
                a:id="@+id/TextView03"
                style="@style/calcRowLabel"
                a:layout_height="wrap_content"
                a:text="@string/bc_calculation_row_label"/>

        <EditText
                a:id="@+id/BCST"
                style="@style/currencyInput"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"/>

        <EditText
                a:id="@+id/BCFR"
                style="@style/percentageInput"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
            a:id="@+id/LinearLayout6"
            style="@style/linearLayoutRow"
            a:layout_width="fill_parent"
            a:layout_height="37sp"
            a:layout_below="@+id/LinearLayout5">

        <TextView
                a:id="@+id/textView1"
                style="@style/calcRowLabel"
                a:layout_height="wrap_content"
                a:text="@string/dc_calculation_row_label"/>

        <EditText
                a:id="@+id/DCST"
                style="@style/inputValue"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"/>

        <EditText
                a:id="@+id/DCFR"
                style="@style/currencyInput"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
            a:id="@+id/LinearLayout8"
            style="@style/linearLayoutRow"
            a:layout_width="fill_parent"
            a:layout_height="37sp"
            a:layout_below="@+id/LinearLayout6">

        <TextView
                a:id="@+id/spacer2"
                style="@style/calcRowLabel"
                a:layout_height="wrap_content"/>

        <TextView
                a:id="@+id/lblVendorIncPci"
                style="@style/rightAlignLabel"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"
                a:text="@string/vendor_inc_pci"/>

        <EditText
                a:id="@+id/FIncPciRate"
                style="@style/currencyInput"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"/>

    </LinearLayout>

    <LinearLayout
            a:id="@+id/LinearLayout7"
            style="@style/linearLayoutRow"
            a:layout_width="fill_parent"
            a:layout_height="37sp"
            a:layout_below="@+id/LinearLayout8"
            >

        <TextView
                a:id="@+id/spacer3"
                style="@style/calcRowLabel"
                a:layout_height="wrap_content"/>

        <TextView
                a:id="@+id/lblVendorTerminal"
                style="@style/rightAlignLabel"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"
                a:text="@string/vendor_terminal"/>

        <EditText
                a:id="@+id/VendorTerminal"
                style="@style/currencyInput"
                a:layout_width="0dp"
                a:layout_height="wrap_content"
                a:layout_weight="1"/>

    </LinearLayout>

</RelativeLayout>
